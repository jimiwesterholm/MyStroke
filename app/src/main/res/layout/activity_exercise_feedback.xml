<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:bind="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.jimi.mystroke.activities.ExerciseFeedbackActivity" >

    <data>
        <import type="android.view.View" />
        <variable name="onClick" type="android.view.View.OnClickListener"/>
    </data>

<android.support.design.widget.CoordinatorLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <include layout="@layout/content_exercise_feedback"
        bind:onClick="@{onClick}"/>

</android.support.design.widget.CoordinatorLayout>
</layout>